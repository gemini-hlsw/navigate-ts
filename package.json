    "primereact": "^8.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "reactflow": "^11.9.3",
    "subscriptions-transport-ws": "^0.11.0"
