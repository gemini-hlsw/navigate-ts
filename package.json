{
  "name": "navigate-ts",
  "description": "Monorepo for navigate typescript projects",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@10.10.0",
  "license": "BSD-3-Clause",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gemini-hlsw/navigate-ui.git"
  },
  "scripts": {
    "ui": "pnpm --filter navigate-ui",
    "lint": "pnpm lint:prettier && pnpm -r lint:eslint",
    "lint:prettier": "prettier --check .",
    "prepare": "node .husky/install.js"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "eslint": "9.25.1",
    "eslint-import-resolver-typescript": "4.3.4",
    "eslint-plugin-import-x": "4.11.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.1"
  },
  "lint-staged": {
    "*.{js,mjs,ts,tsx,jsx,css,md,json,yml}": "prettier --write"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client"
    ]
  }
}
