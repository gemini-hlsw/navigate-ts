{
  "name": "navigate-ui",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@9.12.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gemini-hlsw/navigate-ui.git"
  },
  "scripts": {
    "dev": "vite",
    "tsc:watch": "tsc -w",
    "codegen:watch": "graphql-codegen --config tasks/codegen.ts --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "./deploy.sh",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "codegen": "graphql-codegen --config tasks/codegen.ts",
    "prepare": "node .husky/install.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@apollo/client": "3.11.8",
    "clsx": "2.1.1",
    "graphql": "16.9.0",
    "jotai": "2.10.1",
    "lucuma-core": "0.105.4",
    "lucuma-ui-css": "0.118.1",
    "primeicons": "7.0.0",
    "primereact": "10.8.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.27.0",
    "reactflow": "11.11.4",
    "subscriptions-transport-ws": "0.11.0",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.13.0",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/client-preset": "4.3.3",
    "@graphql-typed-document-node/core": "3.2.0",
    "@parcel/watcher": "2.4.1",
    "@swc-jotai/debug-label": "0.1.1",
    "@swc-jotai/react-refresh": "0.2.0",
    "@swc/core": "1.6.13",
    "@types/eslint__js": "8.42.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/uuid": "10.0.0",
    "@vitejs/plugin-react-swc": "3.7.0",
    "@vitest/browser": "2.1.3",
    "@vitest/coverage-v8": "2.1.3",
    "@vitest/ui": "2.1.3",
    "eslint": "9.13.0",
    "eslint-plugin-react": "7.37.1",
    "globals": "15.11.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "lucuma-schemas": "0.104.0",
    "playwright": "1.48.2",
    "prettier": "3.3.3",
    "sass": "1.80.4",
    "typescript": "5.6.3",
    "typescript-eslint": "8.10.0",
    "vite": "5.4.9",
    "vite-plugin-mkcert": "1.17.6",
    "vitest": "2.1.3",
    "vitest-browser-react": "0.0.1"
  },
  "lint-staged": {
    "*.{js,mjs,ts,tsx,jsx,css,md,json,yml}": "prettier --write"
  }
}
