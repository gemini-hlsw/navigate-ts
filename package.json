{
  "name": "navigate-ui",
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@9.12.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gemini-hlsw/navigate-ui.git"
  },
  "scripts": {
    "dev": "vite",
    "tsc:watch": "tsc -w",
    "codegen:watch": "graphql-codegen --config tasks/codegen.ts --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "./deploy.sh",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "codegen": "graphql-codegen --config tasks/codegen.ts",
    "prepare": "node .husky/install.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@apollo/client": "3.11.10",
    "clsx": "2.1.1",
    "graphql": "16.9.0",
    "jotai": "2.10.2",
    "lucuma-core": "0.107.0",
    "lucuma-ui-css": "0.122.0",
    "primeicons": "7.0.0",
    "primereact": "10.8.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.28.0",
    "reactflow": "11.11.4",
    "subscriptions-transport-ws": "0.11.0",
    "uuid": "11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/client-preset": "4.5.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@parcel/watcher": "2.5.0",
    "@types/eslint__js": "8.42.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/uuid": "10.0.0",
    "@vitejs/plugin-react-swc": "3.7.1",
    "@vitest/browser": "2.1.5",
    "@vitest/coverage-v8": "2.1.5",
    "@vitest/ui": "2.1.5",
    "eslint": "9.14.0",
    "eslint-plugin-react": "7.37.2",
    "globals": "15.12.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "lucuma-schemas": "0.107.1",
    "playwright": "1.48.2",
    "prettier": "3.3.3",
    "sass": "1.81.0",
    "typescript": "5.6.3",
    "typescript-eslint": "8.13.0",
    "vite": "5.4.11",
    "vite-plugin-mkcert": "1.17.6",
    "vitest": "2.1.5",
    "vitest-browser-react": "0.0.3"
  },
  "lint-staged": {
    "*.{js,mjs,ts,tsx,jsx,css,md,json,yml}": "prettier --write"
  }
}
