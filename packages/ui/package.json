{
  "name": "navigate-ui",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gemini-hlsw/navigate-ui.git",
    "directory": "packages/ui"
  },
  "scripts": {
    "dev": "vite",
    "tsc:watch": "tsc -w",
    "codegen:watch": "graphql-codegen --config tasks/codegen.ts --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint:eslint": "eslint .",
    "codegen": "graphql-codegen --config tasks/codegen.ts"
  },
  "devDependencies": {
    "@apollo/client": "3.13.8",
    "@fortawesome/pro-regular-svg-icons": "6.7.2",
    "@fortawesome/pro-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/client-preset": "4.8.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@parcel/watcher": "2.5.1",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react-swc": "3.9.0",
    "@vitest/browser": "3.1.2",
    "@vitest/coverage-v8": "3.1.2",
    "@xyflow/react": "12.6.0",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "graphql": "16.11.0",
    "jotai": "2.12.3",
    "lucuma-core": "0.125.0",
    "lucuma-schemas": "0.131.1",
    "lucuma-ui-css": "0.140.0",
    "playwright": "1.52.0",
    "primereact": "10.9.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.5.2",
    "sass": "1.87.0",
    "subscriptions-transport-ws": "0.11.0",
    "vite": "6.3.3",
    "vite-plugin-mkcert": "1.17.8",
    "vitest": "3.1.2",
    "vitest-browser-react": "0.1.1"
  },
  "bundlemon": {
    "baseDir": "./dist/navigate-ui",
    "pathLabels": {
      "hash": "[a-zA-Z0-9\\-_]+"
    },
    "files": [
      {
        "path": "assets/*-<hash>.{js,css}"
      }
    ],
    "reportOutput": [
      "github"
    ]
  }
}
