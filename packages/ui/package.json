{
  "name": "navigate-ui",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gemini-hlsw/navigate-ts.git",
    "directory": "packages/ui"
  },
  "scripts": {
    "dev": "vite",
    "tsc:watch": "tsc -w",
    "codegen:watch": "graphql-codegen --config tasks/codegen.ts --watch",
    "prebuild": "pnpm run codegen",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "lint:eslint": "eslint .",
    "codegen": "graphql-codegen --config tasks/codegen.ts"
  },
  "devDependencies": {
    "@apollo/client": "3.13.8",
    "@fortawesome/pro-regular-svg-icons": "6.7.2",
    "@fortawesome/pro-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@graphql-codegen/cli": "catalog:",
    "@graphql-codegen/client-preset": "4.8.2",
    "@graphql-typed-document-node/core": "3.2.0",
    "@parcel/watcher": "catalog:",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react-swc": "3.10.2",
    "@vitest/browser": "3.2.2",
    "@vitest/coverage-v8": "3.2.2",
    "@xyflow/react": "12.7.0",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "graphql": "catalog:",
    "jotai": "2.12.5",
    "lucuma-core": "catalog:",
    "lucuma-schemas": "0.141.0",
    "lucuma-ui-css": "0.144.0",
    "navigate-schema": "workspace:*",
    "navigate-server-schema": "0.1.0-20250528-280ea998",
    "playwright": "1.52.0",
    "primereact": "10.9.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.6.2",
    "sass": "1.89.2",
    "subscriptions-transport-ws": "0.11.0",
    "vite": "6.3.5",
    "vite-plugin-mkcert": "1.17.8",
    "vitest": "3.2.2",
    "vitest-browser-react": "0.2.0"
  },
  "bundlemon": {
    "baseDir": "./dist",
    "pathLabels": {
      "hash": "[a-zA-Z0-9\\-_]+"
    },
    "files": [
      {
        "path": "assets/*-<hash>.{js,css}"
      }
    ],
    "reportOutput": [
      "github"
    ]
  }
}
