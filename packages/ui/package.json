{
  "name": "navigate-ui",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gemini-hlsw/navigate-ts.git",
    "directory": "packages/ui"
  },
  "scripts": {
    "dev": "vite",
    "tsc:watch": "tsc -w",
    "codegen:watch": "graphql-codegen --config tasks/codegen.ts --watch 'src/gql/**/*.ts' --watch '../schema/src/*.graphql'",
    "prebuild": "pnpm run codegen",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "lint:eslint": "eslint .",
    "codegen": "graphql-codegen --config tasks/codegen.ts"
  },
  "devDependencies": {
    "@apollo/client": "3.13.9",
    "@fortawesome/pro-regular-svg-icons": "7.0.0",
    "@fortawesome/pro-solid-svg-icons": "7.0.0",
    "@fortawesome/react-fontawesome": "3.0.0",
    "@graphql-codegen/cli": "catalog:",
    "@graphql-codegen/client-preset": "4.8.3",
    "@graphql-typed-document-node/core": "3.2.0",
    "@parcel/watcher": "catalog:",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "5.0.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@xyflow/react": "12.8.3",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "graphql": "catalog:",
    "jotai": "2.13.1",
    "lucuma-core": "catalog:",
    "lucuma-schemas": "catalog:",
    "lucuma-ui-css": "0.156.2",
    "navigate-schema": "workspace:*",
    "navigate-server-schema": "catalog:",
    "playwright": "1.54.2",
    "primereact": "10.9.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.8.1",
    "sass": "1.90.0",
    "subscriptions-transport-ws": "0.11.0",
    "vite": "7.1.3",
    "vite-plugin-mkcert": "1.17.8",
    "vitest": "3.2.4",
    "vitest-browser-react": "1.0.1"
  },
  "bundlemon": {
    "baseDir": "./dist",
    "pathLabels": {
      "hash": "[a-zA-Z0-9\\-_]+"
    },
    "files": [
      {
        "path": "assets/*-<hash>.{js,css}"
      }
    ],
    "reportOutput": [
      "github"
    ]
  }
}
