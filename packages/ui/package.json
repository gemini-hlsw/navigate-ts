{
  "name": "navigate-ui",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gemini-hlsw/navigate-ts.git",
    "directory": "packages/ui"
  },
  "scripts": {
    "dev": "vite",
    "tsc:watch": "tsc -w",
    "codegen:watch": "graphql-codegen --config tasks/codegen.ts --watch 'src/gql/**/*.ts' --watch '../configs/src/graphql/*.graphql'",
    "prebuild": "pnpm run codegen",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "lint:eslint": "eslint .",
    "codegen": "graphql-codegen --config tasks/codegen.ts"
  },
  "devDependencies": {
    "@apollo/client": "4.0.9",
    "@fortawesome/pro-regular-svg-icons": "7.1.0",
    "@fortawesome/react-fontawesome": "3.1.0",
    "@graphql-codegen/cli": "catalog:",
    "@graphql-codegen/client-preset": "5.1.1",
    "@graphql-typed-document-node/core": "3.2.0",
    "@parcel/watcher": "catalog:",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "@vitest/browser-playwright": "4.0.6",
    "@xyflow/react": "12.9.2",
    "babel-plugin-react-compiler": "1.0.0",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "graphql": "catalog:",
    "graphql-ws": "6.0.6",
    "jotai": "2.15.1",
    "lucuma-core": "catalog:",
    "lucuma-schemas": "catalog:",
    "lucuma-ui-css": "0.175.0",
    "navigate-configs": "workspace:*",
    "playwright": "1.56.1",
    "primereact": "10.9.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.5",
    "sass": "1.93.3",
    "vite": "7.1.12",
    "vite-plugin-mkcert": "1.17.9",
    "vitest": "4.0.6",
    "vitest-browser-react": "2.0.2"
  },
  "bundlemon": {
    "baseDir": "./dist",
    "pathLabels": {
      "hash": "[a-zA-Z0-9\\-_]+"
    },
    "files": [
      {
        "path": "assets/*-<hash>.{js,css}"
      }
    ],
    "reportOutput": [
      "github"
    ]
  }
}
